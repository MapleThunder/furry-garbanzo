{
  "name": "sick-fits-backend",
  "version": "1.0.0",
  "description": "the backend of sick fits",
  "scripts": {
    "start": "nodemon -e js,graphql -x node src/index.js",
    "dev": "nodemon -e js,graphql -x node --inspect src/index.js",
    "test": "jest",
    "refresh-db": "npx prisma migrate save --name=init --experimental; npx prisma migrate up --experimental",
    "get-schema": "npx prisma generate; node --trace-warnings src/generator.js",
    "dbref": "npm run refresh-db; npm run get-schema"
  },
  "dependencies": {
    "@nexus/schema": "^0.16.0",
    "@prisma/cli": "2.7.0",
    "@prisma/client": "2.7.0",
    "babel-preset-env": "^1.7.0",
    "bcryptjs": "2.4.3",
    "cookie-parser": "^1.4.3",
    "dotenv": "6.0.0",
    "graphql": "14.6.0",
    "graphql-cli": "^2.16.7",
    "graphql-fields": "^2.0.3",
    "graphql-yoga": "1.16.2",
    "jsonwebtoken": "8.3.0",
    "nexus-plugin-prisma": "^0.20.0",
    "nodemailer": "^4.6.8",
    "nodemon": "^1.18.7",
    "npm-run-all": "^4.1.5",
    "prisma-binding": "2.1.6",
    "stripe": "^6.12.1"
  },
  "license": "MIT",
  "repository": {
    "url": ""
  },
  "babel": {
    "presets": [
      "env"
    ]
  },
  "devDependencies": {
    "@prisma/cli": "^2.15.0"
  }
}
